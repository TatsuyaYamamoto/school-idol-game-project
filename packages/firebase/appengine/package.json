{
  "name": "appengine",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "deploy": "                         gcloud app deploy app.yaml            --project school-idol-game-development",
    "deploy:pro": "     yarn confirm && gcloud app deploy app-production.yaml --project school-idol-game-production",
    "deploy:cron": "                    gcloud app deploy cron.yaml           --project school-idol-game-development",
    "deploy:cron:pro": "yarn confirm && gcloud app deploy cron.yaml           --project school-idol-game-production",
    "start": "node app/index.js",
    "lint": "repo-tools lint",
    "pretest": "npm run lint",
    "system-test": "repo-tools test app",
    "test": "npm run system-test",
    "e2e-test": "repo-tools test deploy",
    "confirm": "../../../production-confirm.js"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.20.1",
    "@slack/client": "^4.8.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^2.3.0"
  },
  "cloud-repo-tools": {
    "test": {
      "app": {
        "msg": "Hello, world!"
      }
    },
    "requiresKeyFile": true,
    "requiresProjectId": true
  }
}
