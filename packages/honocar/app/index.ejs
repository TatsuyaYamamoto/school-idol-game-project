<html>
<head>
    <% if (noIndex) { %>
        <meta name=”robots” content=”noindex,follow”/>
    <% } %>

    <meta charset="utf-8"/>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="description" content="<%= description %>"/>
    <meta name="keyword" content="ラブライブ！,LoveLive！,高坂穂乃果, ホノカチャン, ゲーム,フリゲ,フリーゲーム">

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%= trackingCode %>"></script>
    <script>
      window.dataLayer = window.dataLayer || [];

      function gtag() {
        dataLayer.push(arguments);
      }

      gtag('js', new Date());
      gtag('config', '<%= trackingCode %>');
    </script>

    <script>
      window.addEventListener("error", function(e) {
        console.error(e);
        gtag("event", "exception", {description: e.message, fatal: true});
      });
    </script>

    <script src="https://code.createjs.com/createjs-2015.11.26.min.js"></script>

</head>
<body>
<div id="main">
    <canvas id="gameScrean"></canvas>
</div>
</body>
</html>