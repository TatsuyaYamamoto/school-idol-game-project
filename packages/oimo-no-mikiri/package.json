{
  "name": "oimo-no-mikiri",
  "version": "1.2.2",
  "description": "LoveLive fun game! Outclass an oimo.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --watch",
    "clean": "rimraf dist",
    "minify:image": "imagemin dist/assets/image/*.png --out-dir=dist/assets/image",
    "build": "npm run clean && webpack",
    "build:pro": "npm run clean && NODE_ENV=production webpack && npm run minify:image",
    "deploy:dev": "node scripts/deploy.js",
    "lint": "tslint --format stylish --project .",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TatsuyaYamamoto/oimo-no-mikiri.git"
  },
  "sFactory": {
    "deployPath": "oimo"
  },
  "keywords": [
    "lovelive",
    "oimo"
  ],
  "author": "Tatsuya Yamamoto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TatsuyaYamamoto/oimo-no-mikiri/issues"
  },
  "homepage": "https://github.com/TatsuyaYamamoto/oimo-no-mikiri#readme",
  "devDependencies": {
    "@types/google.analytics": "^0.0.36",
    "@types/i18next": "^8.4.2",
    "@types/i18next-browser-languagedetector": "^2.0.0",
    "@types/mousetrap": "^1.5.34",
    "@types/node": "^8.0.28",
    "@types/pixi.js": "^4.5.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "ora": "^1.3.0",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.6.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.20.2",
    "ts-loader": "^2.3.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "vinyl-fs": "^2.4.4",
    "vinyl-ftp": "^0.6.0"
  },
  "dependencies": {
    "@sokontokoro/mikan": "^0.0.1",
    "animejs": "^2.0.2",
    "autobind-decorator": "^2.1.0",
    "firebase": "^4.10.1",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.0.0",
    "mousetrap": "^1.6.1",
    "pixi.js": "^4.5.4",
    "query-string": "^5.1.0",
    "spin.js": "^3.1.0",
    "sweetalert2": "^7.15.0",
    "tingle.js": "^0.13.0",
    "tippy.js": "^2.2.3",
    "whatwg-fetch": "^2.0.3"
  }
}
