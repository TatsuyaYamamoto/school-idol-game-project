{
  "name": "yamidori",
  "version": "1.2.0",
  "description": "LoveLive fun game! Darkside nabe with cheese cake.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --watch",
    "clean": "rm -rf dist",
    "build": "    yarn clean && webpack",
    "build:pro": "yarn clean && NODE_ENV=production webpack",
    "deploy": "                    yarn build     && gsutil -m rsync -d -r dist/ gs://games-dev.sokontokoro-factory.net/yamidori/",
    "deploy:pro": "yarn confirm && yarn build:pro && gsutil -m rsync -d -r dist/ gs://games.sokontokoro-factory.net/yamidori/",
    "confirm": "../../production-confirm.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TatsuyaYamamoto/yamidori"
  },
  "keywords": [
    "lovelive",
    "kotori",
    "nabe"
  ],
  "author": "Tatsuya Yamamoto",
  "license": "ISC",
  "devDependencies": {
    "@types/i18next": "^8.4.2",
    "@types/node": "^8.0.28",
    "@types/pixi.js": "^4.5.3",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@types/i18next-browser-languagedetector": "^2.0.0",
    "animejs": "^2.0.2",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.0.0",
    "pixi-sound": "^1.4.2",
    "pixi.js": "^4.5.4",
    "sweetalert": "^2.0.3"
  }
}
