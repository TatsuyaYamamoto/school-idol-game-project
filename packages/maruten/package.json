  "scripts": {
    "clean": "rm -rf dist",
    "start": "yarn clean && webpack-serve",
    "build": "    yarn clean &&                     webpack",
    "build:pro": "yarn clean && NODE_ENV=production webpack",
    "deploy": "                    yarn build     && gsutil -m rsync -d -r dist/ gs://games-dev.sokontokoro-factory.net/maruten/",
    "deploy:pro": "yarn confirm && yarn build:pro && gsutil -m rsync -d -r dist/ gs://games.sokontokoro-factory.net/maruten/",
    "confirm": "../../production-confirm.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "alertify": "^0.3.0",
    "createjs": "^1.0.1"
  }
