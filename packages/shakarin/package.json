{
  "name": "shakarin",
  "version": "1.5.0",
  "homepage": "https://github.com/TatsuyaYamamoto/shakarin",
  "author": "TatsuyaYamamoto <yamamoto.tatsuya@fujixerox.co.jp>",
  "description": "js simple game, thanks lovelive",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "yarn clean && webpack-dev-server",
    "build:dev": "yarn clean &&                     webpack",
    "build:pro": "yarn clean && NODE_ENV=production webpack",
    "deploy:dev": "                gsutil -m rsync -d -r dist/ gs://games-dev.sokontokoro-factory.net/shakarin/ && gsutil setmeta -h 'Cache-Control:no-cache' gs://games-dev.sokontokoro-factory.net/shakarin/**/*.html",
    "deploy:pro": "yarn confirm && gsutil -m rsync -d -r dist/ gs://games.sokontokoro-factory.net/shakarin/     && gsutil setmeta -h 'Cache-Control:no-cache' gs://games.sokontokoro-factory.net/shakarin/**/*.html",
    "confirm": "../../production-confirm.js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "alertify": "^0.3.0",
    "createjs": "^1.0.1"
  }
}
